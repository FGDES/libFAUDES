// dot output generated by libFAUDES vGenerator
digraph "Generator" {
  rankdir=LR
  node [shape=circle];

  // initial states
  dot_dummyinit_1 [shape=none, label="", width="0.0", height="0.0" ];
  dot_dummyinit_1 -> "s1";

  // mstates
  "s3" [shape=doublecircle];

  // rest of stateset
  "s2";

  // events
  "a" [style="invis", attr="COfA"];
  "b" [style="invis", attr="cOfA"];

  // transition relation
  "s1" -> "s2" [label="a"];
  "s2" -> "s2" [label="a"];
  "s2" -> "s3" [label="b"];
}
