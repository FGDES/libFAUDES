// dot output generated by libFAUDES TpdGenerator
digraph "Generator" {
  rankdir=LR
  "" [shape = none 
        label = < 
  M=(Q, &Sigma; , &Gamma; , s1 , square, &delta;, Qm) <br align="LEFT"/>
  &Sigma; = &Sigma;c &cup; &Sigma;uc <br align="LEFT"/>
  &Sigma;c = { a }  <br align="LEFT"/>
  &Sigma;uc = { b } <br align="LEFT"/>
  &Gamma; = { dot, square } <br align="LEFT"/>
                >];

  node [shape=circle];

  // initial states
  dot_dummyinit_1 [shape=none, label="", width="0.0", height="0.0" ];
  dot_dummyinit_1 -> "s1";

  // stackbottom
  "square" [style="invis", attr="stackbottom"];

  // mstates
  "s1" [shape=doublecircle];
  "s2" [shape=doublecircle];
  "s3" [shape=doublecircle];

  // rest of stateset

  // events
  "a" [style="invis", attr="COfA"];
  "b" [style="invis", attr="cOfA"];

  // transition relation
  "s1" -> "s2" [label="a,[square][dot,square]" ];
  "s2" -> "s3" [label="b,[dot][dot]" style="dashed" ];
}
